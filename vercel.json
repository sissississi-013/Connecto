{
  "buildCommand": "npm run build",
  "devCommand": "npm run dev",
  "installCommand": "npm install",
  "framework": "nextjs",
  "regions": ["sfo1"],
  "env": {
    "NEXTAUTH_URL": "@nextauth-url",
    "NEXTAUTH_SECRET": "@nextauth-secret",
    "GOOGLE_CLIENT_ID": "@google-client-id",
    "GOOGLE_CLIENT_SECRET": "@google-client-secret",
    "TELNYX_API_KEY": "@telnyx-api-key",
    "TELNYX_CONNECTION_ID": "@telnyx-connection-id",
    "MEMVERGE_API_URL": "@memverge-api-url",
    "MEMVERGE_API_KEY": "@memverge-api-key",
    "APERTUREDATA_HOST": "@aperturedata-host",
    "APERTUREDATA_PORT": "@aperturedata-port",
    "APERTUREDATA_USERNAME": "@aperturedata-username",
    "APERTUREDATA_PASSWORD": "@aperturedata-password",
    "COMET_API_KEY": "@comet-api-key",
    "COMET_PROJECT_NAME": "@comet-project-name",
    "COMET_WORKSPACE": "@comet-workspace",
    "OPENAI_API_KEY": "@openai-api-key"
  }
}
